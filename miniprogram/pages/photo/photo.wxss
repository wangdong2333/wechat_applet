/* pages/photo/photo.wxss */
page{
  height: 100%;
}
#container{
  margin: 0px;
  padding: 0px;
  position: relative;
  background: transparent;
}
#container .bg{
  position: absolute;
  z-index: -1;
  width: 100%;
  height: 100vh;
}

.title{
  position: absolute;
  top: 200rpx;
  left:  50%;
  transform: translateX(-50%);
  font-size: 24px;
  color: #efefef;
}
.img-box{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 500rpx;
}
.img-box image{
  width: 330rpx;
  height: 330rpx;
  opacity: .7;
}